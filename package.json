{
  "name": "griego-monorepo",
  "private": true,
  "workspaces": [
    "api",
    "web"
  ],
  "scripts": {
    "install:all": "npm install",
    "dev": "concurrently \"npm run dev -w api\" \"npm run dev -w web\"",
    "build": "npm run build -ws",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@playwright/test": "^1.41.0",
    "typescript": "^5.3.3"
  }
}
